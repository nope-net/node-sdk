{
  "name": "@nope-net/sdk",
  "version": "1.1.0",
  "description": "TypeScript SDK for the NOPE safety API - risk classification for conversations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "safety",
    "ai",
    "risk",
    "classification",
    "mental-health",
    "crisis",
    "llm",
    "chat"
  ],
  "author": "NOPE <support@nope.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nope-net/node-sdk.git"
  },
  "homepage": "https://nope.net",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
